-  indexPopulation?: number
-): DemographicIndicatorFragment => {
-  return {
-    __typename: 'DemographicIndicatorNode',
-    id: row.id,
-    name: row.name,
-    baseYear: row.baseYear,
-    basePopulation: indexPopulation ?? row.basePopulation,
-    populationPercentage: row.percentage ?? 0,
