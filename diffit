-            UpdateInboundShipmentStatus::Delivered | UpdateInboundShipmentStatus::Verified,
-        ) => true,
-        _ => false,
+        new_invoice_status_index >= InvoiceStatus::Delivered.index()
+            && invoice_status_index < InvoiceStatus::Delivered.index()
+    } else {
+        false
     }
