+
+    fn get_asset_catalogue_property(
+        &self,
+        connection: &StorageConnection,
+        id: String,
+    ) -> Result<Option<AssetCataloguePropertyRow>, RepositoryError> {
+        get_asset_catalogue_property(connection, id)
+    }
