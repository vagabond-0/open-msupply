+    let property_ids = AssetCatalogueItemPropertyRepository::new(&ctx.connection)
+        .query(Some(
+            AssetCataloguePropertyItemFilter::new().catalogue_item_id(EqualFilter::equal_to(&id)),
+        ))?
+        .into_iter()
