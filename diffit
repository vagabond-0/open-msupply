-  if (row.id === GENERAL_POPULATION_ID) {
-    updatedRow = { ...updatedRow, [0]: indexValue ?? 0 };
-  }
-  updatedRow = { ...updatedRow, basePopulation: indexValue ?? 0 };
-
-  return updatedRow;
