-
-  // only update numeric entries
-  const rowNumberKeys = Object.keys(row).filter(
-    key =>
-      !isNaN(parseFloat(key)) &&
-      !(row.id === GENERAL_POPULATION_ID && parseFloat(key) == 0)
-  );
