+pub fn check_line_does_not_exist(
+    connection: &StorageConnection,
+    id: &str,
+) -> Result<bool, RepositoryError> {
+    let result = InvoiceLineRowRepository::new(connection).find_one_by_id(id);
+
+    match result {
+        Err(RepositoryError::NotFound) => Ok(true),
+        Err(error) => Err(error),
+        Ok(_) => Ok(false),
+    }
+}
